    "scripts": {
        "docker:build": "docker-compose build",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "symfony:run": "docker exec -it default-php",
        "symfony:console": "npm run symfony:run bin/console",
        "symfony:install": "npm run symfony:run composer install",
        "symfony:migrate": "npm run symfony:console doctrine:migrations:migrate --no-interaction --allow-no-migration",
        "symfony:fixtures": "npm run symfony:console doctrine:fixtures:load -n"
    }
}
